<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--namesapce对应 mapper接口的全限定符-->
<mapper namespace="com.ptt.spzx.order.mapper.OrderLogMapper">
<insert id="insert" useGeneratedKeys="true" keyProperty="id">
    insert into order_log (
    id,
    order_id,
    operate_user,
    process_status,
    note
    ) values (
    #{id},
    #{orderId},
    #{operateUser},
    #{processStatus},
    #{note}
    )
</insert>
</mapper>